 {\r\n          if (error.response) {\r\n            console.log(error.response.data);\r\n          }\r\n        });\r\n\r\n      console.log(response.data);\r\n    } catch {\r\n      console.log(\"wena\");\r\n    }\r\n    setPost(false)\r\n  };\r\n  \r\n  const cercana = function(array,date){\r\n    let arr = []\r\n    array.forEach(el =>{\r\n      if(dayjs(el).isAfter(date)){\r\n        arr.push(el)\r\n      }\r\n    })\r\n    let index = nearest(arr,date)\r\n    return(arr[index])\r\n\r\n  }\r\n\r\n  const validationSchema = yup.object({\r\n    evento: yup\r\n      .string(\"Ingresa nombre del evento\")\r\n      .required(\"el nombre del evento es obligatorio\"),\r\n    participantes: yup\r\n      .number(\"Ingrese la cantidad de posibles participantes\")\r\n      .integer(\" el número debe ser entero\")\r\n      .moreThan(5,\"el mínimo de participantes debe ser 5\")\r\n      .required(\"la cantidad de participantes es obligatoria\"),\r\n    horaIni: yup\r\n      .date(\"ingrese la fecha y hora de la solicitud de reserva\")\r\n      .required(\"La fecha y hora de inicio es obligatoria\"),\r\n    horaTer: yup\r\n      .date(\"ingrese la hora de finalización de la solicitud reserva\")\r\n      .required(\"La hora de finalización es obligatoria\"),\r\n  });\r\n\r\n  const formik = useFormik({\r\n    initialValues: {\r\n      evento: \"\",\r\n      horaIni: \"\",\r\n      horaTer: \"\",\r\n      participantes: \"\",\r\n    },\r\n    validationSchema,\r\n    onSubmit: (values) => {\r\n      console.log(values);\r\n      HandleSubmit(values);\r\n      formik.resetForm({ values: \"\" });\r\n      handleClose();\r\n    },\r\n  });\r\n\r\n  return (\r\n    <Dialog open={open} onClose={handleClose} maxWidth=\"md\" fullWidth>\r\n    <DialogTitle>Nueva Solicitud</DialogTitle>\r\n    <DialogContent>\r\n      <DialogContentText />\r\n    <form onSubmit={formik.handleSubmit}>\r\n      <Box sx={{ display: \"flex\", flexDirection: \"column\" }}>\r\n        \r\n        <TextField\